<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Edit Changelist...">
        <Item>
          <IdeaActionID>ChangesView.Rename</IdeaActionID>
          <ShortCut>'F2' or 'Shift+F6'</ShortCut>
          <Description>Edit Changelist...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'Shift+F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12' or 'Alt+T'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Key Promoter X">
        <Item>
          <IdeaActionID>ActivateKeyPromoterXToolWindow</IdeaActionID>
          <Description>Key Promoter X</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>